    "@types/node": "12.12.8",
    "@types/ramda": "0.26.21",
    "@types/supertest": "2.0.8",
    "@types/uuid": "3.4.5",
    "@types/validator": "10.11.2",
    "jest": "24.9.0",
    "nodemon": "1.19.1",
