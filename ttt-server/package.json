    "@types/jest": "26.0.0",
    "@types/jsonwebtoken": "8.5.0",
    "@types/lodash": "4.14.159",
    "@types/node": "14.0.27",
    "@types/uuid": "8.3.0",
    "@types/validator": "13.1.0",
    "@types/ws": "7.2.6",
    "apollo-datasource": "0.7.2",
    "apollo-datasource-rest": "0.9.3",
    "apollo-server": "2.16.1",
    "apollo-server-express": "2.16.1",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "graphql": "15.3.0",
    "jest": "24.9.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.19",
    "nodemon": "2.0.4",
    "reflect-metadata": "0.1.13",
    "sqlite3": "5.0.0",
    "subscriptions-transport-ws": "0.9.17",
    "ts-jest": "26.2.0",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "typeorm": "0.2.25",
    "typescript": "3.9.7",
    "uuid": "8.3.0"
