{
  "name": "@grancalavera/ttt-server",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec ts-node -r dotenv/config src/index.ts",
    "start:clean": "rm -f store.sqlite && yarn start",
    "postbuild": "cp src/schema.graphql dist/",
    "build": "tsc",
    "typecheck": "tsc --watch --noEmit",
    "test": "jest",
    "sandbox": "nodemon --watch 'src/**/*.ts' --exec 'ts-node'"
  },
  "dependencies": {
    "apollo-cache-inmemory": "1.6.3",
    "apollo-datasource": "0.6.1",
    "apollo-datasource-rest": "0.6.1",
    "apollo-server": "2.8.1",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "graphql": "14.4.2",
    "graphql-import": "0.7.1",
    "isemail": "3.2.0",
    "lodash": "4.17.15",
    "sequelize": "5.15.0",
    "sqlite3": "4.1.0",
    "subscriptions-transport-ws": "0.9.16",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.27",
    "@types/dotenv": "6.1.1",
    "@types/graphql": "14.2.3",
    "@types/jest": "24.0.16",
    "@types/lodash": "4.14.136",
    "@types/node": "12.7.2",
    "@types/sqlite3": "3.1.5",
    "@types/uuid": "3.4.5",
    "@types/validator": "10.11.2",
    "jest": "24.8.0",
    "nodemon": "1.19.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.3"
  }
}
