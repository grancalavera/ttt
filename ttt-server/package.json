{
  "name": "@grancalavera/ttt-server",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec ts-node -r dotenv/config src/index.ts",
    "start:clean": "rm -f store.sqlite && yarn start",
    "start:test-client": "nodemon --watch 'test-client/index.ts' --exec ts-node --project tsconfig-test-client.json test-client/index.ts",
    "postbuild": "cp src/schema.graphql dist/",
    "build": "tsc",
    "build:test-client": "tsc --project tsconfig-test-client.json",
    "typecheck": "tsc --watch --noEmit",
    "test": "jest",
    "sandbox": "nodemon --watch 'src/**/*.ts' --exec 'ts-node'"
  },
  "dependencies": {
    "apollo-cache-inmemory": "1.6.3",
    "apollo-client": "2.6.4",
    "apollo-datasource": "0.6.1",
    "apollo-datasource-rest": "0.6.1",
    "apollo-link": "1.2.12",
    "apollo-link-context": "1.0.18",
    "apollo-link-http": "1.5.15",
    "apollo-link-ws": "1.0.18",
    "apollo-server": "2.8.1",
    "apollo-utilities": "1.3.2",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "graphql": "14.4.2",
    "graphql-import": "0.7.1",
    "graphql-tag": "2.10.1",
    "isomorphic-fetch": "2.2.1",
    "isomorphic-ws": "4.0.1",
    "lodash": "4.17.15",
    "sequelize": "5.21.2",
    "sqlite3": "4.1.0",
    "subscriptions-transport-ws": "0.9.16",
    "uuid": "3.3.2",
    "ws": "7.1.2"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.27",
    "@types/dotenv": "6.1.1",
    "@types/graphql": "14.2.3",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "24.0.17",
    "@types/lodash": "4.14.137",
    "@types/node": "12.7.2",
    "@types/uuid": "3.4.5",
    "@types/validator": "10.11.2",
    "@types/ws": "6.0.2",
    "jest": "24.8.0",
    "nodemon": "1.19.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.3"
  }
}
