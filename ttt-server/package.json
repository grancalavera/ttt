{
  "name": "@grancalavera/ttt-server",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec ts-node -r dotenv/config -r tsconfig-paths/register src/index.ts",
    "start:clean": "rm -f ../*.sqlite && yarn start",
    "build": "tsc",
    "build:test-client": "tsc --project tsconfig-test-client.json",
    "typecheck": "tsc --watch --noEmit",
    "test": "jest",
    "sandbox": "nodemon --watch 'src/**/*.ts' --exec 'ts-node'"
  },
  "dependencies": {
    "@grancalavera/ttt-api": "^0.0.0",
    "@grancalavera/ttt-core": "^0.0.0",
    "apollo-datasource": "0.6.3",
    "apollo-datasource-rest": "0.6.6",
    "apollo-server": "2.8.1",
    "apollo-server-express": "2.9.7",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "graphql": "14.5.8",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.15",
    "reflect-metadata": "0.1.13",
    "sequelize": "5.21.2",
    "sqlite3": "4.1.0",
    "typeorm": "0.2.20",
    "typescript": "3.7.3",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "1.4.2",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.1",
    "@types/graphql": "14.5.0",
    "@types/jest": "24.0.22",
    "@types/jsonwebtoken": "8.3.5",
    "@types/lodash": "4.14.149",
    "@types/node": "12.12.8",
    "@types/uuid": "3.4.3",
    "@types/validator": "10.11.2",
    "@types/ws": "6.0.2",
    "jest": "24.9.0",
    "nodemon": "1.19.1",
    "ts-jest": "24.1.0",
    "ts-node": "8.3.0",
    "tsconfig-paths": "3.9.0"
  }
}
